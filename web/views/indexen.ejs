<!DOCTYPE html>
<html>

<% include ./header.ejs %>
<body>
  <h1>
    <%= title %>
  </h1>
  <p>Welcome to <%= title %>
  </p>
  <input type="text" id="keyword" placeholder="search keyword" autocomplete="off">
  <ul id="list">
    <% list.forEach(video=> { %>
      <li><img src="<%= video.thumbnail %>" title="<%= video.title %>" loading="lazy" />
        <div style="margin: 3px 0">
          <%= video.pubdate %>
        </div>
        <a href="https://youtu.be/<%= video.videoId %>" alt="<%= video.title %>">
          <%= video.title %>
        </a>
      </li>
      <% }) %>
  </ul>
  <hr />
  <% include ./aside.ejs %>
  <hr />
  <hr />
  <hr />
</body>
